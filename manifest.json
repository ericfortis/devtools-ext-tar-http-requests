{
  "name": "save-requests-ext",
  "description": "Extension for downloading a tar with the HTTP requests",
  "manifest_version": 3,
  "version": "1.0",
  "devtools_page": "devtools.html",
  "permissions": [
    "system.memory"
  ],
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": [
        "node_modules/@gera2ld/tarjs/dist/index.js"
      ]
    }
  ]
}
